}

.approval .body .approvalContent .deleteTips {
	width: 416px;
	height: 260px;
	border-radius: 6px;
	border: 1px solid #6f8eb2;
	position: absolute;
	top: 230px;
	left: 380px;
	background-color: #fff;
	overflow: hidden;
}

.approval .body .approvalContent .deleteTips .title {
	width: 416px;
	height: 50px;
	line-height: 50px;
	text-align: center;
	background-color: #445670;
	font-size: 20px;
	color: #fff;
}

.approval .body .approvalContent .deleteTips .content {
	width: 416px;
	height: 210px;
}

.approval .body .approvalContent .deleteTips .content h4 {
	display: block;
	font-size: 24px;
	color: #445670;
	width: 416px;
	height: 30px;
	line-height: 30px;
	text-align: center;
	margin-top: 30px;
}

.approval .body .approvalContent .deleteTips .content .deleteTips-btn {
	width: 416px;
	height: 150px;
}

.approval .body .approvalContent .deleteTips .content .deleteTips-btn span {
	float: left;
	display: block;
	width: 90px;
	height: 34px;
	line-height: 34px;
	text-align: center;
	font-size: 20px;
	margin-top: 70px;
	border-radius: 6px;
	cursor: pointer;
}

.approval .body .approvalContent .deleteTips .content .deleteTips-btn span:first-child {
	margin-left: 84px;
	margin-right: 46px;
	background-color: #569ff1;
	border: 1px solid #569ff1;
}

.approval .body .approvalContent .deleteTips .content .deleteTips-btn span:last-child {
	border: 1px solid #6f8eb2;
	color: #6f8eb2;
}