/* 审批 */
.approval{
	position: relative;
}

.approval .body .approvalContent { 
	width: 1150px; 
	height: 712px; 
	overflow: hidden;
	position: relative;
}

.approval .body .approvalContent .list {
	width: 1135px;
	height: 698px;
	margin-top: 15px;
	margin-left: 15px;
}

.approval .body .approvalContent .list thead tr {
	display: inline-block;
	width: 1120px;
	height: 58px;
	border-bottom: 1px solid #dae4e8;
	border-top: 1px solid #dae4e8;
}

.approval .body .approvalContent .list thead tr td {
	height: 59px;
	border-left: 1px solid #dae4e8;
	color: #6b7f9b;
	font-size: 20px;
	text-align: center;
	line-height: 59px;
}

.approval .body .approvalContent .list thead tr td:nth-child(1) {
	width: 190px;
}

.approval .body .approvalContent .list thead tr td:nth-child(2) {
	width: 700px;
}

.approval .body .approvalContent .list thead tr td:nth-child(3) {
	width: 70px;
}

.approval .body .approvalContent .list thead tr td:nth-child(4) {
	width: 162px;
	border-right: 1px solid #dae4e8;
}

.approval .body .approvalContent .list tbody tr {
	display: inline-block;
	width: 1120px;
	height: 58px;
}

.approval .body .approvalContent .list tbody tr td {
	height: 59px;
	border-left: 1px solid #dae4e8;
	border-bottom: 1px solid #dae4e8;
}

.approval .body .approvalContent .list tbody tr td span {
	display: block;
	color: #253137;
	font-size: 20px;
	text-align: center;
	height: 59px;
	line-height: 59px;
}

.approval .body .approvalContent .list tbody tr td:nth-child(1) {
	width: 190px;
	border-bottom: 1px solid #fff;
}

.approval .body .approvalContent .list tbody tr td:nth-child(1):hover {
	background-color: #8fa8cc;
	cursor: pointer;
}

.approval .body .approvalContent .list tbody tr td.active {
	background-color: #8fa8cc;
}

.approval .body .approvalContent .list tbody tr td:nth-child(1):hover span {
	color: #fff;
}

.approval .body .approvalContent .list tbody tr td.active span {
	color: #fff;
}

/* .approval .body .approvalContent .list tbody tr td:nth-child(1) span {
	width: 185px;
} */

.approval .body .approvalContent .list tbody tr td:nth-child(2) {
	width: 700px;
}

.approval .body .approvalContent .list tbody tr td:nth-child(3) {
	width: 70px;
}

.approval .body .approvalContent .list tbody tr td:nth-child(3) a {
	width: 70px;
	height: 58px;
	display: block;
	overflow: hidden;
}

.approval .icon_look {
	width: 24px;
	height: 24px;
	display: block;
	background: url(../image/icon_look.png) no-repeat;
	margin: 17px auto;
}

.approval .body .approvalContent .list tbody tr td:nth-child(4) {
	width: 162px;
	border-right: 1px solid #dae4e8;
}

.approval .body .approvalContent .list tbody tr td:nth-child(4) div {
	width: 162px;
	height: 58px;
}

.approval .body .approvalContent .list tbody tr td:nth-child(4) div a {
	display: inline-block;
	font-size: 20px;
	color: #fff;
	height: 32px;
	line-height: 32px;
	margin-top: 13px;
	text-align: center;
	border-radius: 4px;
	margin-left: 6px;
}

.approval .body .approvalContent .list tbody tr td:nth-child(4) div a:first-child {
	width: 66px;
	background-color: #569ff1;
}

.approval .body .approvalContent .list tbody tr td:nth-child(4) div a:last-child {
	width: 70px;
	background-color: #fe7b7b;
}

.approval .body .approvalContent .list tbody tr:last-child td:nth-child(1) {
	width: 190px;
	border-bottom: 1px solid #dae4e8;
}

.approval .windowPage {
	position: absolute;
	width: 824px;
	height: 605px;
	top: 64px;
	left: 330px;
	border-radius: 6px;
	border:1px solid #6f8eb2;
	overflow: hidden;
}

.approval .windowPage .header {
	width: 824px;
	height: 46px;
	background-color: #7ea4cf;
}

.approval .icon_close {
	width: 30px;
	height: 30px;
	display: block;
	margin-top: 8px;
	margin-right: 8px;
	float: right;
	background: url(../image/icon_close.png) no-repeat;
}

.approval .icon_close:hover {
	cursor: pointer;
}

.approval .windowPage .body {
	width: 824px;
	height: 560px;
	background-color: rgba(0,0,0,0.76);
}

.approval .windowPage .body iframe {
	width: 100%;
	height: 100%;
}
